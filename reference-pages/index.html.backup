      font-size: 12px;
      color: #fff;
      background: #1b74e4;
      padding: 2px 6px;
      border-radius: 10px;
      margin-left: 6px;
    }

    /* Meta row */
    #fbcomments .meta {
      display: flex;
      align-items: center;
      flex-wrap: wrap;
      gap: 8px;
      color: var(--gray);
      font: 12px/18px Helvetica, Arial, sans-serif;
      margin-top: 2px;
    }

    #fbcomments .meta>*+*::before {
      content: "·";
      margin: 0 2px 0 0;
      color: var(--gray);
    }

    #fbcomments .blue {
      color: var(--fb-action);
      cursor: pointer;
    }

    #fbcomments .blue:hover {
      text-decoration: underline;
    }

    .icon {
      width: 14px;
      height: 14px;
      vertical-align: -2px;
    }

    .like-btn,
    .likecount {
      display: inline-flex;
      align-items: center;
      gap: 4px;
    }

    /* Replies */
    ul.replies {
      margin: 8px 0 0 48px;
      list-style: none;
      padding-left: 12px;
      border-left: 1px dotted #d3d6db;
    }

    ul.replies>li {
      margin: 8px 0;
    }

    /* Small screens */
    @media (max-width:420px) {
      #fbcomments {
        max-width: 100%;
        margin: 12px 12px !important;
      }

