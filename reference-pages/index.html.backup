    #fbcomments h3 {
      font: 700 14px Helvetica, Arial, sans-serif;
      color: var(--fb-blue);
      margin: 0;
    }

    #fbcomments p {
      margin: 4px 0;
      font: 14px/20px Helvetica, Arial, sans-serif;
      color: #050505;
    }

