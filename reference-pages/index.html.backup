      const SUB_PARAM_NAME = "sub16"; // The sub parameter you want to pass

      function getVariantId() {
        try {
          return window?.funnelish?.pageId || null;
        } catch (e) {
          return null;
        }
      }

      function appendSubToUrl(paramName, value) {
        if (!value) return false;
        const url = new URL(window.location.href);

        // Skip if already exists
        if (url.searchParams.get(paramName)) return true;

        url.searchParams.set(paramName, value);
        window.history.replaceState({}, "", url.toString());
        console.log(`✅ Added ${paramName}=${value} to URL`);
        return true;
      }

      function loadRedTrackScript() {
        const script = document.createElement("script");
        script.src = REDTRACK_URL;
        script.async = true;
        document.head.appendChild(script);
        console.log("🚀 RedTrack script loaded with current URL params");
      }

      // --- MAIN EXECUTION ---
      const checkInterval = setInterval(() => {
        const variantId = getVariantId() || 'None';
        if (variantId) {
          clearInterval(checkInterval);
          const added = appendSubToUrl(SUB_PARAM_NAME, variantId);
          if (added) loadRedTrackScript();
        }
      }, 300);
    })();
